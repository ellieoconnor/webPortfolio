{
  "name": "web-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage --coverageDirectory='./coverage'",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,scss,html}\"",
    "format:check": "prettier --check 'src/**/*.{ts,js,json,css,scss,html}'",
    "lint": "eslint . --ext .js,.ts"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular/animations": "^21.0.3",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@fortawesome/angular-fontawesome": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.19.1",
    "jest-preset-angular": "^14.6.0",
    "marked": "^15.0.6",
    "rxjs": "~7.8.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.3.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@types/marked": "^6.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "sass": "^1.80.5",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.9.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}